{
    "$schema": "https://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "title": "Root Schema",
    "type": "object",
    "default": {},
    "required": [
        "title",
        "salary",
        "equity"
    ],
    "properties": {
        "title": {
            "title": "The title Schema",
            "type": "string",
            "default": "",
            "examples": [
                "janitor"
            ]
        },
        "salary": {
            "title": "The salary Schema",
            "type": "integer",
            "default": 0,
            "examples": [
                20000
            ]
        },
        "equity": {
            "title": "The equity Schema",
            "type": "number",
            "default": 0.0,
            "examples": [
                0.01
            ]
        }
    },
    "examples": [{
        "title": "janitor",
        "salary": 20000,
        "equity": 0.01
    }]
}